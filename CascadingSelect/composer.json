{
    "name": "depentatselect",
    "description": "An extension to add cascading select as a field type within Bolt",
    "type": "bolt-extension",
    "keywords": [
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [{"name": "Facundo Chaer", "email": "facuchaer@gmail.com"}],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extensions\\Chaerf\\CascadingSelect\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Chaerf\\CascadingSelect\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Chaerf\\CascadingSelect\\CascadingSelectExtension"
    }
}
